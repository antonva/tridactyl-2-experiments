{
  "manifest_version": 2,
  "name": "saulrh's Tridactyl 2 Experiments",
  "version": "0.0.1",
  // "background": { },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content.js" ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'unsafe-eval' 'self'; object-src 'self'",
  "permissions": [
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "tridactyl-2-experiments@saulrh.com",
      "strict_min_version": "66.0"
    }
  }
}
